    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Movement and gravity

mov_direction = keyboard_check(vk_right) - keyboard_check(vk_left);
// -1 : going left
// +1 : going right
// 0 ; standing still


// Sprite modifications


if mov_direction != 0 { // If weâ€™re moving
    image_xscale = mov_direction;
    sprite_index = sp_player_running;
    image_speed = 5; //RUNNING ANIMATION SPEED
}
else {
//    sprite_index = sp_player_idle;
//    image_speed = 5; //IDLE ANIMATION SPEED
    image_index = 3;
    image_speed = 0;
}

// Capping max gravity
if (global.vsp &lt; MAX_VSP) {
   global.vsp += GRAV;
}


// Stuff to do on the ground
if not place_free(x, y + 1) {
    global.g = "down";
    image_angle = 0;  
    global.vsp = -JUMP_SPEED * keyboard_check_pressed(vk_space);
    if not safe_spawn {
        safe_spawn = true;
        cp_x = x;
        cp_y = y;
        cp_room = room;
    }
}
else if not place_free(x, y - 1) {
    global.g = "up";
    image_angle = 180;
    global.vsp = -JUMP_SPEED * keyboard_check_pressed(vk_space);
    if not safe_spawn {
        safe_spawn = true;
        cp_x = x;
        cp_y = y;
        cp_room = room;
    }
}
else if not place_free(x + 1, y) {
    global.g = "right";
    image_angle = 90;
    global.vsp = -JUMP_SPEED * keyboard_check_pressed(vk_space);
    if not safe_spawn {
        safe_spawn = true;
        cp_x = x;
        cp_y = y;
        cp_room = room;
    }
}
else if not place_free(x - 1, y) {
    global.g = "left";
    image_angle = 270;
    global.vsp = -JUMP_SPEED * keyboard_check_pressed(vk_space);
    if not safe_spawn {
        safe_spawn = true;
        cp_x = x;
        cp_y = y;
        cp_room = room;
    }
}
else {// Stuff to do airborne
    image_speed = 0;
    image_index = 3;
}
    


// Downward gravity
if (global.g == "down") {


    // Apply gravity

    if (place_free(x, y +global.vsp)) { // If the frame won't end into a wall
        y +=global.vsp;
    }
    else {
        while (place_free(x, y + sign(global.vsp))) { // Move until just before contact
            y += sign(global.vsp);
        }
    }
    
    mov = MOV_SPEED * mov_direction;
    while (mov != 0) {                        
        if not place_free(x + mov_direction, y) {
        
            // We're on a wall and need to get on it
            y -= 1;
            mov = 0;
        
        }
        else if place_free(x, y + 1) and not place_free(x - mov_direction, y + 1) { 
            
            // We're falling and need to stick to the wall
            y += 1;
            mov = 0;
            // Automatic gravity will do the rest        
        }
        else {
            mov -= mov_direction; // Keep moving
            x += mov_direction;
        }
    }
}

// Upward gravity
else if (global.g == "up") {
    
    // Apply gravity
    if not place_free(x, y - 1) { // If there's a floor beneath us
       global.vsp = -JUMP_SPEED * keyboard_check_pressed(vk_space);
    }
    if (place_free(x, y - global.vsp)) { // If the frame won't end into a wall
        y -= global.vsp;
    }
    else {
        while (place_free(x, y - sign(global.vsp))) { // Move until just before contact
            y -= sign(global.vsp);
        }
    }
    
    mov = MOV_SPEED * mov_direction;
    while (mov != 0) {                        
        if not place_free(x - mov_direction, y) {
        
            // We're on a wall and need to get on it
            y += 1;
            mov = 0;
        
        }
        else if place_free(x, y - 1) and not place_free(x + mov_direction, y - 1) { 
            
            // We're falling and need to stick to the wall
            y -= 1;
            mov = 0;
            // Automatic gravity will do the rest       
        }
        else {
            mov -= mov_direction; // Keep moving
            x -= mov_direction;
        }
    }
}

// Rightward gravity
else if (global.g == "right") {
      
    
    // Apply gravity
    if not place_free(x + 1, y) { // If there's a floor beneath us
       global.vsp = -JUMP_SPEED * keyboard_check_pressed(vk_space);
    }
    if (place_free(x + global.vsp,y)) { // If the frame won't end into a wall
        x += global.vsp;
    }
    else {
        while (place_free(x + sign(global.vsp), y)) { // Move until just before contact
            x += sign(global.vsp);
        }
    }
    
    mov = MOV_SPEED * mov_direction;
    while (mov != 0) {                        
        if not place_free(x, y - mov_direction) {
        
            // We're on a wall and need to get on it
            x -= 1;
            mov = 0;
        
        }
        else if place_free(x + 1, y) and not place_free(x + 1, y + mov_direction) { 
            
            // We're falling and need to stick to the wall
            x += 1;
            mov = 0;
            // Automatic gravity will do the rest   
        }
        else {
            mov -= mov_direction; // Keep moving
            y -= mov_direction;
        }
    }
}
                
// Leftward gravity
else if (global.g == "left") {
       
    // Apply gravity
    if not place_free(x - 1, y) { // If there's a floor beneath us
       global.vsp = -JUMP_SPEED * keyboard_check_pressed(vk_space);
    }
    if (place_free(x - global.vsp,y)) { // If the frame won't end into a wall
        x -= global.vsp;
    }
    else {
        while (place_free(x - sign(global.vsp), y)) { // Move until just before contact
            x -= sign(global.vsp);
        }
    }
    
    mov = MOV_SPEED * mov_direction;
    while (mov != 0) {                        
        if not place_free(x, y + mov_direction) {
        
            // We're on a wall and need to get on it
            x += 1;
            mov = 0;
        
        }
        else if place_free(x - 1, y) and not place_free(x - 1, y - mov_direction) { 
            
            // We're falling and need to stick to the wall
            x -= 1;
            mov = 0;
            // Automatic gravity will do the rest        
        }
        else {
            mov -= mov_direction; // Keep moving
            y += mov_direction;
        }
    }
}

else {
    show_error("Gravity incorrectly set", false);
}
</string>
          </argument>
        </arguments>
      </action>
