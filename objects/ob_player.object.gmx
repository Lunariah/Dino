if (keyboard_check(vk_left) ^^ keyboard_check(vk_right))
{
    if (airborne) {
        image_speed = 0;
        image_index = 1;
    }
    else {
        sprite_index = sp_player_running;
        image_speed = RUNNING_ANIM_SPEED;
    }
}
else
{
    sprite_index = sp_player_idle;
    image_speed = IDLE_ANIM_SPEED;
}

// The sprite is flipped and mirror in the movement step
