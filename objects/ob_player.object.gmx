        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Init

// Constants and variables

GRAV = 0.4;
MAX_VSP = 15;
JUMP_SPEED = 5;
MOV_SPEED = 6;
IDLE_ANIMATION_SPEED = 5;
RUNNING_ANIMATION_SPEED = 5;

g = side.down;
vsp = 0;
rescues[0] = false;
rescues[1] = false;
rescues[2] = false;
rescues[3] = false;
rescues[4] = false;


/// Initial sprite status

image_speed = 0;

if g == side.down
    image_angle = 0;
else if g == side.up
    image_angle = 180;
else if g == side.right
    image_angle = 90;
else if g == side.left
    image_angle = 270; 
    
///Declare checkpoint variables

cp_room = room_1;
cp_x = 64;
cp_y = 415;

safe_spawn = false;

//death_anim_toggled = false;
</string>
          </argument>
