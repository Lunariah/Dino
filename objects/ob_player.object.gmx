        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Movement and gravity

EDGE_CORRECTION = 12; // Corrects sprite position when the rectangular hitbox gets flipped // Originally 8 // Try 7

switch (g) 
{
    case side.down:
        x_grav = 0;
        y_grav = 1;
        break;
        
    case side.up:
        x_grav = 0;
        y_grav = -1;
        break;
        
    case side.left:
        x_grav = -1;
        y_grav = 0;
        break;
        
    case side.right:
        x_grav = 1;
        y_grav = 0;
        break;
}

if not place_free(x + x_grav, y + y_grav)
{
    // Stuff to do on the ground    
    if(keyboard_check_pressed(vk_space)) {
        vsp = -JUMP_SPEED;
        jumping = true;
        airborne = true; // Used for animation
    }
    else  airborne = false;
    if not safe_spawn Save_Checkpoint(); // Save a checkpoint the first time we touch the ground in any room
}
else
{
    // Stuff to do airborne
    airborne = true;
}


// Gravity + Jump

if (vsp &lt; MAX_VSP) {
    vsp += GRAV;
}

var x_spd = x_grav * vsp;
var y_spd = y_grav * vsp;

if (place_free(x + x_spd, y + y_spd)) { // If the frame won’t end into a wall
    x += x_spd;
    y += y_spd;
}
else {
    while(place_free(x + sign(x_spd), y + sign(y_spd))) { // Move until just before contact
        x += sign(x_spd);
        y += sign(y_spd);
    }
}

// Player movement
// (perpendicular to gravity)

var mov_direction = keyboard_check(vk_right) - keyboard_check(vk_left);
var x_dir = mov_direction * y_grav;
var y_dir = -mov_direction * x_grav;
var x_mov = MOV_SPEED * x_dir;
var y_mov = MOV_SPEED * y_dir;

while (x_mov != 0 or y_mov != 0) // Both directions are taken into account, but one of them will always be == 0
{
    if not place_free(x + x_dir, y + y_dir)
    {
        // We’re in front of a wall and need to get on it
        y -= y_grav;
        x -= x_grav;
        x_mov = 0;
        y_mov = 0;
    }   
    
    else if place_free(x + x_grav, y + y_grav) and not place_free(x - x_dir + x_grav, y - y_dir + y_grav)
    //   if we’re falling                      and we weren’t falling a pixel ago
    {
        // We’re on an edge and need to stick down the wall
        x += EDGE_CORRECTION * x_grav;
        y += EDGE_CORRECTION * y_grav;
        if (!airborne) {
            x += 15 * x_grav;
            y += 15 * y_grav;
        }
        x_mov = 0;
        y_mov = 0;
    }
    
    else
    {
        x += x_dir;
        x_mov -= x_dir;
        y += y_dir;
        y_mov -= y_dir;
    }
}

// Gravity switch + sprite transformations

//y -= y_grav * x_grav * 7; // Was meant to be the hitbox correction but should always be y -= 0. Remove if stable

if not place_free(x, y + 1) {
    g = side.down;
    image_angle = 0;
    if(x_grav != 0)
        y -= 7; // Correction for the rectangular hitbox // Try using EDGE_CORRECTION instead
}
else if not place_free(x, y - 1) {
    g = side.up;
    image_angle = 180;
    if(x_grav != 0)
        y += 7;
}
else if not place_free(x + 1, y) {
    g = side.right;
    image_angle = 90;
    if(y_grav != 0)
        x -= 7;
}
else if not place_free(x - 1, y) {
    g = side.left;
    image_angle = 270;
    if(y_grav != 0)
        x += 7;
}

if mov_direction != 0
    image_xscale = mov_direction; // Flips the sprite

</string>
